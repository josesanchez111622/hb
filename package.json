  "homepage": "/",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "targets": {
    "js": {
      "source": "static/src/js/main.js",
      "distDir": "static/dist/js/"
    },
    "css": {
      "source": "static/src/css/main.css",
      "distDir": "static/dist/css/"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "resolutions": {
    "webpack": "^5"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "jest": {
    "testTimeout": 10000,
    "maxWorkers": "80%",
    "rootDir": "./",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup-tests.js"
    ],
    "testEnvironment": "jest-environment-jsdom",
    "testRegex": ".*.test.jsx$",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/scripts/config/jest/babelTransform.js",
      "^.+\\.(jpg|png|otf|svg|ttf|woff|woff2|scss)$": "<rootDir>/test/file-transformer.js",
      "^.+\\.css$": "<rootDir>/scripts/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/scripts/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^src/(.*)$": "<rootDir>/src/$1",
      "@src/(.*)": "<rootDir>/src/$1",
      "@test/(.*)": "<rootDir>/test/$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageDirectory": "./src-coverage",
    "reporters": [
      "default",
      [
        "jest-stare",
        {
          "resultDir": "./coverage/html",
          "reportTitle": "jest-stare!",
          "coverageLink": "../lcov-report/index.html"
        }
      ]
    ],
    "coverageThreshold": {
      "global": {
        "statements": 3.7,
        "branches": 0.5,
        "lines": 6.1,
        "functions": 2.4
      }
    },
    "collectCoverage": false
  }
}
